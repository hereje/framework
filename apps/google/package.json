{
    "name": "@juicyllama/app-google",
    "version": "0.0.5",
    "description": "Juicyllama NestJs Google App",
    "author": {
        "name": "JuicyLlama Studio",
        "email": "studio@juicyllama.com",
        "url": "https://juicyllama.com"
    },
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
    "repository": {
        "type": "git",
        "url": "https://github.com/juicyllama/framework/apps/google"
    },
    "bugs": "https://github.com/juicyllama/framework",
    "license": "0BSD",
    "readmeFilename": "README.md",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "*.md"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "start:dev": "tsc -w",
        "build": "tsc",
        "prepare": "pnpm run build",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "test": "jest --detectOpenHandles --forceExit",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "@juicyllama/core": "workspace:*",
        "@juicyllama/utils": "workspace:*",
        "@googlemaps/google-maps-services-js": "^3.3.30"
    },
    "devDependencies": {
        "@juicyllama/dev": "workspace:*"
    },
    "overrides": {
        "axios": "^1.4.0",
        "@googlemaps/google-maps-services-js": {
            "axios": "^0.27.2"
        }
    },
	"eslintConfig": {
		"extends": [
			"./node_modules/@juicyllama/dev/.eslintrc.js"
		]
	},
	"prettier": "./node_modules/@juicyllama/dev/.prettierrc.js",
	"jest": {
		"preset": "./node_modules/@juicyllama/dev/jest.config.js"
	}
}
