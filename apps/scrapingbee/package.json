{
    "name": "@juicyllama/app-scrapingbee",
    "version": "0.0.2",
    "description": "Juicyllama NestJs ScrapingBee App",
    "author": {
        "name": "JuicyLlama Studio",
        "email": "studio@juicyllama.com",
        "url": "https://juicyllama.com"
    },
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
    "repository": {
        "type": "git",
        "url": "https://github.com/juicyllama/framework/apps/scrapingbee"
    },
    "bugs": "https://github.com/juicyllama/framework",
    "license": "0BSD",
    "readmeFilename": "README.md",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "*.md"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "prebuild": "rimraf dist",
        "prepare": "pnpm run build",
        "build": "tsc",
        "sandbox": "nest start --config src/sandbox/nest-cli.json --watch ",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "test": "jest --detectOpenHandles --forceExit",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@juicyllama/core": "workspace:*",
        "@juicyllama/utils": "workspace:*",
        "scrapingbee": "^1.7.3"
    },
    "devDependencies": {
        "@juicyllama/dev": "workspace:*"
    }
}
