{
    "name": "@juicyllama/app-apilayer",
    "version": "0.0.34",
    "description": "Juicyllama ApiLayer App",
    "author": {
        "name": "JuicyLlama Studio",
        "email": "studio@juicyllama.com",
        "url": "https://juicyllama.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/juicyllama/framework/apps/apilayer"
    },
    "bugs": "https://github.com/juicyllama/framework",
    "license": "0BSD",
    "readmeFilename": "README.md",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "*.md"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "start:dev": "tsc -w",
        "prebuild": "rimraf dist",
        "build": "tsc",
        "prepare": "pnpm run build",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "test": "pnpm run prebuild && jest --runInBand",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@juicyllama/core": "workspace:*",
        "@juicyllama/utils": "workspace:*"
    },
    "devDependencies": {
        "@juicyllama/dev": "workspace:*"
    },
    "eslintConfig": {
        "extends": [
            "./node_modules/@juicyllama/dev/.eslintrc.js"
        ]
    },
    "prettier": "./node_modules/@juicyllama/dev/.prettierrc.js"
}
