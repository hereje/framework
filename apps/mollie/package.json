{
    "name": "@juicyllama/app-mollie",
    "version": "0.0.42",
    "description": "Juicyllama NestJs Mollie App",
    "author": {
        "name": "JuicyLlama Studio",
        "email": "studio@juicyllama.com",
        "url": "https://juicyllama.com"
    },
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
    "repository": {
        "type": "git",
        "url": "https://github.com/juicyllama/framework/apps/mollie"
    },
    "bugs": "https://github.com/juicyllama/framework",
    "license": "0BSD",
    "readmeFilename": "README.md",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "*.md"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "prebuild": "rimraf dist",
        "prepare": "pnpm run build",
        "build": "tsc",
        "start:dev": "tsc -w",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "test": "jest --runInBand --detectOpenHandles --forceExit",
        "test:clear": "npx jest --clearCache",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "devDependencies": {
        "@juicyllama/dev": "workspace:*"
    },
    "dependencies": {
        "@juicyllama/billing": "workspace:*",
        "@juicyllama/core": "workspace:*",
        "@juicyllama/utils": "workspace:*",
        "@mollie/api-client": "^3.7.0"
    }
}
