{
    "name": "@juicyllama/app-aws",
    "version": "0.0.34",
    "description": "Juicyllama NestJs AWS App",
    "author": {
        "name": "JuicyLlama Studio",
        "email": "studio@juicyllama.com",
        "url": "https://juicyllama.com"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/juicyllama/framework/tree/main/apps/aws"
    },
    "bugs": "https://github.com/juicyllama/framework",
    "license": "0BSD",
    "readmeFilename": "README.md",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "*.md"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "prebuild": "rimraf dist",
        "prepare": "pnpm run build",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "test": "export $(grep -v '^#' ../../.env.development) && NODE_ENV=test && jest --runInBand --passWithNoTests",
        "test:clear": "npx jest --clearCache",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prerelease": "npm run format && npm run lint && npm run test && npm run build ",
        "postrelease": "pnpm publish --no-git-checks --access=public",
        "release": "release-it --ci",
        "release:minor": "release-it minor --ci",
        "release:major": "release-it major --ci",
        "releaseit:skip": "release-it --ci"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.418.0",
        "@aws-sdk/client-secrets-manager": "^3.418.0",
        "@aws-sdk/client-sesv2": "^3.418.0",
        "@aws-sdk/client-sns": "^3.418.0",
        "@aws-sdk/lib-storage": "^3.418.0",
        "@juicyllama/core": "workspace:^",
        "@juicyllama/utils": "workspace:^"
    },
    "devDependencies": {
        "@juicyllama/dev": "workspace:^"
    }
}
